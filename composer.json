{
  "name": "thestory/freshmail-module",
  "type": "library",
  "description": "Freshmail API communication module for Zend Framework 3 and Zend Expressive",
  "license": "Apache 2.0",
  "require": {
    "php": ">=5.6.0",
    "zendframework/zend-config": "^2.6",
    "zendframework/zend-json": "^3.0",
    "zendframework/zend-http": "^2.5",
    "zendframework/zend-modulemanager": "^2.7",
    "zendframework/zend-servicemanager": "^3.1"
  },
  "autoload": {
    "psr-0": {
      "Freshmail\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "FreshmailTest\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5"
  }
}
